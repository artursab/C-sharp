//Optional Method Parametrs


class Program
{
    static int Sum(int a, int b, bool enableLogging = false)
    {
        int result = a + b;

        if (enableLogging)
        {
            Console.WriteLine("Value of variable a = " + a);
            Console.WriteLine("Value of variable b = " + b);
            Console.WriteLine("Result of summ = " + result);
        }

        return result;
    }



    static void Main(string[] args)
    {
        int result = Sum(5,2);
    }
}
